{
    "compilerOptions": {
        "target": "ES2021",
        "module": "CommonJS",
        "lib": [
            "ES2021"
        ],
        "strict": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "inlineSourceMap": true,
        "inlineSources": true,
        "typeRoots": [
            "./node_modules/@types"
        ]
    }
}